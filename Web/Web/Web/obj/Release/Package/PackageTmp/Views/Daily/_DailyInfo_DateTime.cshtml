@model DailyInfo
@{
    DailyTypeExtra dte = Common.DailyTypeExtra.Single(x => x.extraId == Model.extraId);
}
@Form.Controls.DateTime(new Form.DateTime()
{
    Id = "sidebar_extraId_" + Model.extraId,
    IdName = "extraId_" + Model.extraId,
    Title = dte.internalTitle,
    LabelText = dte.internalTitle,
    Required = dte.isRequired,
    Value = Model.entry
})