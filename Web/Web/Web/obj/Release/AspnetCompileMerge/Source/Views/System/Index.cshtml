@{
    ViewBag.Title = "System Setup";
}
<div class="d-flex">
    <div class="skd-table col-12 col-lg-6">
        <div class="card">
            <div class="card-header">
                Session Variables
            </div>
            <div class="card-body skd-table">
                @foreach (string key in Session.Keys)
                {
                    <div class="skd-table-row shade">
                        <div class="col-4 ellipsis">@key</div>
                        <div class="col-8 ellipsis">@Session[key]</div>
                    </div>
                }
            </div>
        </div>
    </div>
    <div class="col-12 col-lg-6">
        <div class="card">
            <div class="card-header">
                Request Cookies
            </div>
            <div class="card-body skd-table">
                @foreach (string key in Request.Cookies.Keys)
                {
                    <div class="skd-table-row shade">
                        <div class="col-4 ellipsis">@key</div>
                        <div class="col-8 ellipsis">@Request.Cookies[key].Value</div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>